            <string>///Estados e movimentacao

//Movimentacao basica
vx+=dir;
vy+=cim;
vx = clamp(vx,-5,5);
vy = clamp(vy,-5,5);
vx*=0.8;
vy*=0.8;


//Estado Principal
switch Estado {
    case Parado:
        if (dir != 0 || cim != 0) {Estado = Anda;}
        ataca();
    break;
    
    case Anda:
        if (dir == 0 &amp;&amp; cim == 0) {Estado = Parado;}
        movimenta_geral(vx,vy);
        define_dir();
        ataca();
    break;
    
    case Ataca:
        //Altera o estado no animation end
        ataca();
        var vel = (image_number - image_index)/image_number;
        var sp = 2;
        var dx = (vx+lengthdir_x(sp,olha*90))*vel;
        var dy = (vy+lengthdir_y(sp,olha*90))*vel;
        
        dx = clamp(dx,-sp,sp);
        dy = clamp(dy,-sp,sp);
        
        movimenta_geral(dx,dy);
    break;

}


depth_atualiza();

</string>
