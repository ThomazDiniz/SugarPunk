            <string>///Codigo dos controles
dir = 0;
cim = 0;

if abs(gamepad_axis_value(0,gp_axislh)) &gt; 0.2{
    dir = gamepad_axis_value(0,gp_axislh);
}
if abs(gamepad_axis_value(0,gp_axislv)) &gt; 0.2{
    cim = gamepad_axis_value(0,gp_axislv);
}

if gamepad_button_check(0,gp_padl){dir--}
if gamepad_button_check(0,gp_padr){dir++;}
if gamepad_button_check(0,gp_padu){cim--;}
if gamepad_button_check(0,gp_padd){cim++;}

pula = gamepad_button_check_pressed(0,gp_face1);
pulando =  gamepad_button_check(0,gp_face1);
dash = gamepad_button_check_pressed(0,gp_face2);
dashando =  gamepad_button_check(0,gp_face2);



//Primario e secundario
primaria_clica = gamepad_button_check_pressed(0,gp_face3);
primaria_segura = gamepad_button_check(0,gp_face3);
primaria_solta = gamepad_button_check_released(0,gp_face3);
secundaria_clica = gamepad_button_check_pressed(0,gp_face4);
secundaria_segura = gamepad_button_check(0,gp_face4);
secundaria_solta = gamepad_button_check_released(0,gp_face4);

//Troca
troca_primaria = gamepad_button_check_pressed(0,gp_shoulderl);
troca_secundaria = gamepad_button_check_pressed(0,gp_shoulderr);


//Se precisar passar esses botoes para outro objeto eh mais facil e deixa o player mais limpo:
with(pai){
    dir = other.dir;
    cim = other.cim;
    //bater = other.bater
    //bater_direct = other.bater_direct
    pula = other.pula;
    pulando = other.pulando;
    dash = other.dash;
    dashando = other.dashando;

    //Primario e secundario
    primaria_clica = other.primaria_clica;
    primaria_segura = other.primaria_segura;
    primaria_solta = other.primaria_solta;
    secundaria_clica = other.secundaria_clica;
    secundaria_segura = other.secundaria_segura;
    secundaria_solta = other.secundaria_solta;

    //Troca
    troca_primaria = other.troca_primaria;
    troca_secundaria = other.troca_secundaria;
}

</string>
